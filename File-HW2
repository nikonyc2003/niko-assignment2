#include<iostream>
using namespace std;

int main(){
        int category;
        cout << "Enter a category of hurricane (1 through 5) :\n";
        cin >> category;
        if (category == 1){
                cout << "The windspeed is 74-95 mph.\n";
        }
        else if (category == 2){
                cout << "The windspeed is 96-100 mph.\n";
        }
        else if (category == 3){
                cout << "The windspeed is 111-130 mph.\n";
        }
        else if (category == 4){
                cout << "The windspeed is 131-155 mph.\n";
        }
        else if (category == 5){
                cout << "The windspeed exceeds 155 mph.\n";
        }
        else {
                cout << "Invalid input.\n";
        }
        return 0;
}
